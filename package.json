{
  "name": "caab-api-certificacao",
  "version": "1.0.0",
  "description": "REST API for Primeira Certificação Google Spreadsheet",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "generate:spec": "tsx scripts/generate-openapi.ts",
    "build": "pnpm generate:spec && tsc",
    "start": "node dist/index.js",
    "dev": "pnpm generate:spec && tsx watch src/index.ts",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "api",
    "google-sheets",
    "certificacao"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "googleapis": "^129.0.0",
    "zod": "^3.22.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "ignoredBuiltDependencies": [
      "@scarf/scarf"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.0",
    "@types/swagger-jsdoc": "^6.0.4",
    "eslint": "^9.39.2",
    "swagger-jsdoc": "^6.2.8",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "typescript-eslint": "^8.55.0"
  }
}
